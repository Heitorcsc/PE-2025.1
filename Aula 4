#include <stdio.h>
#define TAM 30

int main(){
    int soma;
        double media;
            int numeros[TAM];
                for(int i = 0; i < TAM; i += 1){

                        scanf("%d",&numeros[i]);
                                soma += numeros[i];
                                }

                                media = soma / 30;
                                printf("Media =  %2.f\n",media);

                                for(int i = 0;i < TAM; i+= 1){
                                    if (numeros[i] > media){
                                            printf("%d",numeros[i]);
                                                }
                                                }

                                                return 0;
                                                }
                                                